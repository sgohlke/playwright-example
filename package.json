{
  "name": "playwright-example",
  "version": "1.0.0",
  "description": "Example using playwright browser automation",
  "main": "rollthedice.js",
  "scripts": {
    "installBrowsersCI": "playwright install-deps",
    "start": "node rollthedice.js",
    "test": "playwright test obst.spec.js --browser=all --reporter=html",
    "testUI": "playwright test --ui",
    "initScreenshots": "playwright test obst.spec.js --browser=all --update-snapshots",
    "showReport": "playwright show-report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sgohlke/playwright-example.git"
  },
  "keywords": [
    "playwright",
    "browser",
    "automation"
  ],
  "author": "Stefan Gohlke",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sgohlke/playwright-example/issues"
  },
  "homepage": "https://github.com/sgohlke/playwright-example#readme",
  "devDependencies": {
    "@playwright/test": "1.46.1"
  },
  "dependencies": {
    "cross-fetch": "4.0.0",
    "playwright": "1.46.1"
  }
}
